Title = {{Infants Detect Cross-modal Cues to Identity in Speech and Singing}},
Booktitle = {{NEUROSCIENCES AND MUSIC III: DISORDERS AND PLASTICITY}},
Series = {{Annals of the New York Academy of Sciences}},
Year = {{2009}},
Volume = {{1169}},
Pages = {{508-511}},
Note = {{Conference on the Neurosciences and Music III, McGill Univ, Montreal,
   CANADA, JUN 25-28, 2008}},
Abstract = {{Little is known about infants' perception of cross-modal cues to
   identity, but the importance of recognizing familiar individuals makes
   it likely that this skill would be evident early in life. Infants 6-8
   months of age were tested on their ability to link dynamic cross-modal
   cues to the identity of unfamiliar speakers and singers. After exposure
   to speech or singing, infants watched two silent videos, one featuring
   the previously heard speaker or singer. Infants looked significantly
   longer at the video of the person heard previously, which indicates that
   they can match auditory and visual cues to the identity of unfamiliar
   persons.}},
